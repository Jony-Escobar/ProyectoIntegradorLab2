doctype html
html(lang="es")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Atención Médica | #{pagina}

        //- Bootstrap
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous")

        //- Jquery
        script(src='https://code.jquery.com/jquery-3.7.1.js')
        //-script(src='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/js/bootstrap.bundle.min.js')
        script(src='https://cdn.datatables.net/2.1.8/js/dataTables.js')
        script(src='https://cdn.datatables.net/2.1.8/js/dataTables.bootstrap5.js')

        link(src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css")
        link(src="https://cdn.datatables.net/2.1.8/css/dataTables.bootstrap5.css")
        
        //- FullCalendar CSS y JS
        link(href='https://cdn.jsdelivr.net/npm/@fullcalendar/core/main.css' rel='stylesheet')
        link(href='https://cdn.jsdelivr.net/npm/@fullcalendar/daygrid/main.css' rel='stylesheet')
        link(href='https://cdn.jsdelivr.net/npm/@fullcalendar/timegrid/main.css' rel='stylesheet')
        script(src='https://cdn.jsdelivr.net/npm/fullcalendar@6.1.15/index.global.min.js')

        //- Tinymces
        script(src="https://cdn.tiny.cloud/1/txr4xi0lyi05tq66i4ks7yz64n900dv2ukehtij2bpv6q7z2/tinymce/7/tinymce.min.js" referrerpolicy="origin")

        //- Css & Js Local
        block estilos
        
    body
        div(class="contenidoPrincipal")
            block contenido
            block footer
            .container
                footer.d-flex.flex-wrap.justify-content-center.align-items-center.py-3.my-4.border-top
                    p.text-center.mb-0.text-muted &copy; 2024 - Proyecto Integrador - ULP

        //- Scripts al final del body
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous")
        script(src="/js/app.js" type="module")