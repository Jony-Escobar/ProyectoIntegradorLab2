extends layout

block estilos
    link(rel="stylesheet" href="/css/agenda.css")

block contenido
    main
        .container
          header.d-flex.flex-wrap.justify-content-center.py-3.mb-4.border-bottom
            a.d-flex.align-items-center.mb-3.mb-md-0.me-md-auto.text-dark.text-decoration-none(href='/')
              svg.bi.me-2(width='40' height='32')
                use(xlink:href='#bootstrap')
              span.fs-4 Agenda de Fernandito
            ul.nav.nav-pills
              li.nav-item
                a.nav-link.active(href='#' aria-current='page') Home
              li.nav-item
                a.nav-link(href='#') Features
              li.nav-item
                a.nav-link(href='#') Pricing
              li.nav-item
                a.nav-link(href='#') FAQs
              li.nav-item
                a.nav-link(href='#') About

    div(class="container" id="calendar")

    .container
        footer.d-flex.flex-wrap.justify-content-between.align-items-center.py-3.my-4.border-top
          p.col-md-4.mb-0.text-muted &copy; 2024 Company, Inc
          a.col-md-4.d-flex.align-items-center.justify-content-center.mb-3.mb-md-0.me-md-auto.link-dark.text-decoration-none(href='/')
            svg.bi.me-2(width='40' height='32')
              use(xlink:href='#bootstrap')
          ul.nav.col-md-4.justify-content-end
            li.nav-item
              a.nav-link.px-2.text-muted(href='#') Home
            li.nav-item
              a.nav-link.px-2.text-muted(href='#') Features
            li.nav-item
              a.nav-link.px-2.text-muted(href='#') Pricing
            li.nav-item
              a.nav-link.px-2.text-muted(href='#') FAQs
            li.nav-item
              a.nav-link.px-2.text-muted(href='#') About
